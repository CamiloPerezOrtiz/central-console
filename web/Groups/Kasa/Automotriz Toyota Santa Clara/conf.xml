<?xml version='1.0'?>
	<squidguarddest>
			<config>
				<name>Instagram</name>
				<domains>instagram.com</domains>
				<urls>instagram.com/login</urls>
				<expressions>|insta|</expressions>
				<redirect_mode>rmod_int</redirect_mode>
				<redirect>Error</redirect>
				<description>Bloquear_instagram_kasa</description>
				<enablelog>on</enablelog>
			</config>
			<config>
				<name>Facebook</name>
				<domains>facebook.com</domains>
				<urls>facebook.com/login</urls>
				<expressions>|facebook|</expressions>
				<redirect_mode>rmod_int</redirect_mode>
				<redirect>Error</redirect>
				<description>Bloquear_facebook</description>
				<enablelog>on</enablelog>
			</config>
			<config>
				<name>Twitter</name>
				<domains>twitter.com</domains>
				<urls>twitter.com/login</urls>
				<expressions>|twitter|</expressions>
				<redirect_mode>rmod_int</redirect_mode>
				<redirect>Error</redirect>
				<description>Bloquear_twitter</description>
				<enablelog>on</enablelog>
			</config>
	</squidguarddest>