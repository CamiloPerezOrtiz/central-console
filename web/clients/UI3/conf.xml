<?xml version='1.0'?>
	<squidguarddest>
			<config>
				<name>Instagram</name>
				<domains>instagram.com</domains>
				<urls>instagram.com/</urls>
				<expressions>|insta|instagram|</expressions>
				<redirect_mode>rmod_int</redirect_mode>
				<redirect>ERROR</redirect>
				<description>Bloquear_Instagram</description>
				<enablelog>on</enablelog>
			</config>
	</squidguarddest>